  <script src="js/bloxx.js"></script>
  <script src="js/bloxx-animations.js"></script>
  <script>
    // Billing toggle functionality
    document.getElementById('billingToggle').addEventListener('change', function() {
      const priceElements = document.querySelectorAll('[data-monthly-price]');
      priceElements.forEach(function(el) {
        if (this.checked) {
          el.textContent = '$' + el.dataset.annualPrice;
        } else {
          el.textContent = '$' + el.dataset.monthlyPrice;
        }
      }, this);
    });
  </script>
